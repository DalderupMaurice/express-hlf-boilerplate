{
  "name": "express-hlf-example",
  "version": "1.0.0",
  "description": "A Boilerplate application for building REST APIs using express and the fabric SDK.",
  "author": "Maurice Dalderup <Maurice_Dalderup@hotmail.com>",
  "main": "dist/server/index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "scripts": "chmod +x ./scripts/* && chmod +x ../basic-network/*.sh",
    "buildChaincode": "npm run scripts && cd ./scripts && ./buildChaincode.sh",
    "prestart": "npm run scripts && cd ./scripts && ./startNetwork.sh",
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=express-hlf-example:* yarn start",
    "startServer": "gulp serve",
    "startNetwork": "yarn run prestart",
    "lint": "esw *.js server config --color --fix",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "yarn lint",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DalderupMaurice/express-hlf-example.git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate",
    "hyperledger",
    "fabric"
  ],
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-validation": "^1.0.2",
    "express-winston": "^2.5.0",
    "fabric-ca-client": "^1.1.0",
    "fabric-client": "^1.1.0",
    "helmet": "^3.12.0",
    "http-status": "^1.0.1",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.12",
    "morgan": "^1.9.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-watch": "^3.1.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-newer": "^1.4.0",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "husky": "^0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.0.5",
    "run-sequence": "^2.2.1",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "babel-polyfill"
    ],
    "plugins": [
      "add-module-exports"
    ],
    "env": {
      "test": {
        "presets": [
          "env",
          "stage-2",
          "babel-polyfill"
        ],
        "plugins": [
          "add-module-exports"
        ]
      }
    }
  }
}
