{
  "name": "express-hlf-example",
  "version": "1.0.0",
  "description": "A Boilerplate application for building REST APIs using express and the fabric SDK.",
  "author": "Maurice Dalderup <Maurice_Dalderup@hotmail.com>",
  "main": "dist/server/index.js",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.6.0"
  },
  "scripts": {
    "scripts": "chmod +x ./bin/* && chmod +x ../basic-network/*.sh",
    "buildChaincode": "npm run scripts && cd ./scripts && ./buildChaincode.sh",
    "prestart": "npm run scripts && cd ./bin/node && ./startNetwork.sh",
    "start": "gulp serve",
    "start:debug": "cross-env DEBUG=express-hlf-example:* yarn start",
    "startServer": "gulp serve",
    "startNetwork": "yarn run prestart",
    "lint": "esw *.js server config --color --fix",
    "lint:watch": "yarn lint -- --watch",
    "precommit": "lint-staged",
    "prettier": "prettier --print-width=120 --write 'server/**/*.js'",
    "test": "jest --verbose",
    "test:old": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "boom": "7.2.0",
    "build-url": "1.1.1",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "3.1.0",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-jwt": "5.3.1",
    "express-queue": "0.0.12",
    "express-validation": "1.0.2",
    "express-winston": "2.5.1",
    "fabric-ca-client": "1.1.0",
    "fabric-client": "1.1.0",
    "helmet": "3.23.3",
    "http-status": "1.0.1",
    "joi": "13.2.0",
    "jsonwebtoken": "8.2.1",
    "jwt-decode": "2.2.0",
    "method-override": "2.3.10",
    "moment": "2.22.2",
    "mongoose": "5.7.5",
    "morgan": "1.9.1",
    "superagent": "3.8.2",
    "uuid": "3.2.1",
    "winston": "2.4.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-function-bind": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "coveralls": "3.0.3",
    "cross-env": "5.1.6",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-watch": "3.1.5",
    "gulp": "3.9.1",
    "gulp-babel": "7.0.1",
    "gulp-load-plugins": "1.5.0",
    "gulp-newer": "1.4.0",
    "gulp-nodemon": "2.2.1",
    "gulp-sourcemaps": "2.6.4",
    "husky": "0.14.3",
    "istanbul": "1.1.0-alpha.1",
    "jest": "23.1.0",
    "lint-staged": "7.1.3",
    "mocha": "5.2.0",
    "prettier": "1.13.5",
    "run-sequence": "2.2.1",
    "supertest": "3.1.0",
    "supertest-as-promised": "4.0.2"
  },
  "lint-staged": {
    "*.{js,css}": [
      "yarn lint",
      "prettier --print-width=120 --write",
      "git add ."
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DalderupMaurice/express-hlf-example.git"
  }
}
