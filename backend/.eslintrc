{
  "env": {
    "node": true,
    "jest": true,
    "mocha": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": [
    "prettier",
  ],
  "extends": [
    "airbnb-base",
    "prettier",
  ],
  "rules": {
    "prettier/prettier": "error",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true }],
    "consistent-return": ["off"],
    "array-callback-return":"off",
    "no-underscore-dangle": "off"
  }
}
